      "@/services/*": ["./src/services/*"],
      "@/interfaces/*": ["./src/interfaces/*"],
      "@/components/*": ["./src/views/components/*"],
      "@/layouts/*": ["./src/views/layouts/*"],
      "@/pages/*": ["./src/views/pages/*"],
